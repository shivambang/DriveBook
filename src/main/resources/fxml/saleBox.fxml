<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="sb_cust" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookkeeper.SaleBox">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" percentWidth="20.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" percentWidth="5.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" percentWidth="15.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" percentWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" percentWidth="30.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="5.0" percentHeight="25.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" percentHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" percentHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" percentHeight="15.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Date" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
         <font>
            <Font name="Roboto Regular" size="18.0" />
         </font>
      </Label>
      <DatePicker fx:id="sb_date" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.valignment="CENTER" />
      <Label text="ID" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <font>
            <Font name="Roboto Regular" size="18.0" />
         </font>
      </Label>
      <Label text="Name" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <font>
            <Font name="Roboto Regular" size="18.0" />
         </font>
      </Label>
      <Label text="Employee" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="emp_id" editable="false" prefWidth="150.0" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
      <ComboBox fx:id="emp_name" editable="false" prefWidth="350.0" GridPane.columnIndex="4" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
      <GridPane GridPane.columnIndex="1" GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <Label text="Customer" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <font>
            <Font name="Roboto Regular" size="18.0" />
         </font>
      </Label>
      <Label text="ID" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <font>
            <Font name="Roboto Regular" size="18.0" />
         </font>
      </Label>
      <Label text="Name" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <font>
            <Font name="Roboto Regular" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="cust_id" editable="false" prefWidth="150.0" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
      <ComboBox fx:id="cust_name" editable="false" prefWidth="350.0" GridPane.columnIndex="4" GridPane.halignment="LEFT" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
      <Button mnemonicParsing="false" onAction="#sb_done" prefHeight="15.0" prefWidth="55.0" text="Done" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="BOTTOM">
         <font>
            <Font name="Roboto Regular" size="15.0" />
         </font>
      </Button>
   </children>
   <GridPane.margin>
      <Insets bottom="10.0" />
   </GridPane.margin>
</GridPane>
